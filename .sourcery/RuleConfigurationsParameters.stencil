{% for configuration in types.based.RuleConfiguration %}
public extension {{ configuration.name }} {
    var parameters: [ParameterDefinition] {
        return [
          {% for variable in configuration.storedVariables where variable.name|hasSuffix:"Parameter" %}
          {{ variable.name }}{% if not forloop.last %},{% endif %}
          {% endfor %}
        ]
    }
}

{% endfor %}